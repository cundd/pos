{
    "name": "cundd/stairtower",
    "description": "A document storage entirely written in PHP",
    "homepage": "http://stairtower.cundd.net",
    "require": {
        "php": "^7.1",
        "doctrine/annotations": "^1.5.0",
        "doctrine/cache": "^1.7.0",
        "monolog/monolog": "^1.23",
        "php-di/php-di": "^5.4.0",
        "react/react": "~0.4.2",
        "symfony/console": "^3.3.6",
        "symfony/process": "^3.3.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.3.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Corn",
            "email": "info@cundd.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "Cundd\\PersistentObjectStore\\": "Classes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cundd\\PersistentObjectStore\\Tests\\": "Tests/"
        }
    },
    "bin": [
        "bin/console",
        "bin/server"
    ],

    "suggest": {
        "twig/twig": "Provides the standard view implementation for the application framework"
    }
}
